:host {
    .title {
        width: 100%
        height: 40px
        float: left
        background-color: rgb(36,36,36)
        font-size: 18px
        line-height: 40px
        padding-left: 15px
        border-bottom: 1px solid rgb(66,66,66)
    }

    .section{
        width: 100%
        height: auto
        float: left
    }

    .prop {
        height: 24px
        margin-left: 15px
        margin-top: 10px
        margin-right: 15px
        padding-bottom: 5px
        // border-top: 1px solid rgb(40,40,40)
        border-bottom: 1px solid rgb(40,40,40)
        fire-ui-label {
            font-size: 13px
        }
    }

    .itemBox {
        height: 100px
        max-height: 100px
        overflow: hidden
        margin-top: 5px
    }

    .border {
        border: 1px solid rgb(153, 153, 153)
        position: absolute
        max-height: 100px
        left: 191px
        width: 296px

    }

    .items {
        height: 80px
        border: 1px solid rgb(153, 153, 153)
        overflow-x: hidden
        overflow-y: auto
        fire-ui-checkbox {
            margin: 3px
        }
    }
}
